#peograma que solicita as notas do aluno

nota1=int(input("informe sua primeira nota:"))
nota2=int(input('informe a sua segunda nota:'))
media=int

if (nota1>=0) and(nota1<=100) and (nota2>=0) and (nota2<=100):
    print(f'a nota da primeira etapa {nota1} do aluno foi {nota1}')
    print(f"a nota da segunda etapa {nota2} do aluno foi {nota2}")
    media=(nota1*2+nota2*3)/5
    print(f"a media parcial {media} do aluno é {media}")
else:
    print("uma das notas é invalida")
if media>=60:
        print('o aluno foi aprovado')
elif media<=60:
    print("o aluno esta na prova final")
    print("informar notas da prova final")
    nota3=int(input("informe primeira nota:"))
    nota4=int(input('informe a segunda nota:'))
media2=int

if (nota3>=0) and(nota3<=100) and (nota4>=0) and (nota4<=100):
    print(f'a nota da primeira etapa {nota3} do aluno foi {nota3}')
    print(f"a nota da segunda etapa {nota4} do aluno foi {nota4}")
    media2=(nota3*2+nota4*3)/5
    print(f"a media parcial {media2} do aluno é {media2}")
else:
    print("uma das notas é invalida")
if media2>=60:
        print('o aluno foi aprovado')
if media2<media:
    print (f"a media parcial {media} do aluno é {media}")
    print("o aluno foi reprovado! sem outras chances")
